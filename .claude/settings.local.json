{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "CLAUDE_CODE_ENABLE_TELEMETRY": "1",
    "DISABLE_PROMPT_CACHING": "0",
    "MCP_TIMEOUT": "30000",
    "OTEL_METRIC_EXPORT_INTERVAL": "10000",
    "NODE_ENV": "development"
  },
  "includeCoAuthoredBy": false,
  "permissions": {
    "allow": [
      "Bash(tidy:*)",
      "Bash(open http://localhost:8002/frontend/)",
      "Bash(pgrep:*)",
      "Bash(./scripts/status.sh:*)",
      "Bash(curl:*)",
      "Bash(git log:*)",
      "Bash(git add:*)",
      "Bash(python -m pytest test_file_validator.py -v --cov=file_validator --cov-report=term-missing)",
      "Bash(PYTHONPATH=. python -m pytest:*)",
      "Bash(python -m pytest src/processing/test_file_validator.py -v --cov=src.processing.file_validator --cov-report=term-missing)",
      "Bash(test -f src/processing/test_file_validator.py)",
      "Bash(test:*)",
      "Bash(pytest:*)",
      "Bash(python:*)",
      "Bash(git commit:*)",
      "Bash(chmod:*)",
      "Bash(docker ps:*)",
      "Bash(docker restart:*)",
      "Bash(python3:*)",
      "Bash(open /Volumes/tkr-riffic/@tkr-projects/tkr-docusearch/.context-kit/_ref/details_page/documents.svg)",
      "Bash(open:*)",
      "Bash(./scripts/stop-all.sh:*)",
      "Bash(./scripts/start-all.sh:*)",
      "Read(//private/var/folders/**)",
      "Bash(docker logs:*)",
      "Bash(docker inspect:*)",
      "Bash(docker-compose:*)",
      "WebSearch",
      "Bash(source .venv-native/bin/activate)",
      "Bash(pip show:*)",
      "Bash(pip index:*)",
      "WebFetch(domain:github.com)",
      "Bash(pip install:*)",
      "Bash(/Volumes/tkr-riffic/@tkr-projects/tkr-docusearch/.venv-native/bin/pip install:*)",
      "Bash(/Volumes/tkr-riffic/@tkr-projects/tkr-docusearch/.venv-native/bin/pip show:*)",
      "Bash(/Volumes/tkr-riffic/@tkr-projects/tkr-docusearch/.venv-native/bin/pip check:*)",
      "Bash(/Volumes/tkr-riffic/@tkr-projects/tkr-docusearch/.venv-native/bin/python:*)",
      "Bash(cat:*)"
    ],
    "deny": [],
    "ask": []
  },
  "defaultMode": "acceptEdits",
  "additionalDirectories": [
    ".context-kit"
  ]
}
